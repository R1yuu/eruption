complete -c eruption-macro -n "__fish_seen_subcommand_from description" -s l -l lua-file -r -F
complete -c eruption-macro -n "__fish_seen_subcommand_from description" -s h -l help -d 'Print help information'
complete -c eruption-macro -n "__fish_seen_subcommand_from completions" -s h -l help -d 'Print help information'
