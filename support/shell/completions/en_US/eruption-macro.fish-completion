complete -c eruption-macro -n "__fish_use_subcommand" -s v -l verbose -d 'Verbose mode (-v, -vv, -vvv, etc.)'
complete -c eruption-macro -n "__fish_use_subcommand" -s h -l help -d 'Print help information'
complete -c eruption-macro -n "__fish_use_subcommand" -s V -l version -d 'Print version information'
complete -c eruption-macro -n "__fish_use_subcommand" -f -a "add" -d 'Ein Makro hinzufügen'
complete -c eruption-macro -n "__fish_use_subcommand" -f -a "remove" -d 'Ein Makro entfernen'
complete -c eruption-macro -n "__fish_use_subcommand" -f -a "record" -d 'Record a key sequence and save it as a macro'
complete -c eruption-macro -n "__fish_use_subcommand" -f -a "description" -d 'Show or set the description of a specified macro'
complete -c eruption-macro -n "__fish_use_subcommand" -f -a "completions" -d 'Generate shell completions'
complete -c eruption-macro -n "__fish_use_subcommand" -f -a "help" -d 'Print this message or the help of the given subcommand(s)'
complete -c eruption-macro -n "__fish_seen_subcommand_from add" -l description -d 'Specify a description for a rule' -r
complete -c eruption-macro -n "__fish_seen_subcommand_from add" -s e -l enabled -d 'Specify the enabled status of the newly added rule'
complete -c eruption-macro -n "__fish_seen_subcommand_from add" -s h -l help -d 'Print help information'
